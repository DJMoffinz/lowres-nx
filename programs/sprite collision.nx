COPY START(1),LENGTH(1) TO $8000

PALETTE 1,0,4,8,16

REM INIT SPRITES
SPRITE.A 0,(0,,,,3)
SPRITE.A 1,(1,,,,3)
SPRITE.A 2,(1,,,,0)
SPRITE.A 3,(1,,,,0)

DIM X(1),Y(1)
X=74
Y=64
SPRITE 0,X,Y,3
SPRITE 1,120,80,3
SPRITE 2,50,70,1
SPRITE 3,160,130,2
DO
  IF UP(0) THEN Y=Y-1
  IF DOWN(0) THEN Y=Y+1
  IF LEFT(0) THEN X=X-1
  IF RIGHT(0) THEN X=X+1
  SPRITE 0,X,Y,
  IF SPRITE HIT(0) THEN TEXT 0,0,"HIT "+STR$(HIT) ELSE TEXT 0,0,"      "
  WAIT VBL
LOOP



PRINT "HELLO WORLD"

GAMEPAD 1
BG 1
BG COPY 0,0,32,16 TO 0,0

X=100
Y=60
BGX=0
DO
  BGX=BGX+1
  DISPLAY 1,BGX,0
  IF UP(0) THEN Y=Y-1
  IF DOWN(0) THEN Y=Y+1
  IF LEFT(0) THEN X=X-1
  IF RIGHT(0) THEN X=X+1
  SPRITE 0,X,Y,TIMER/4 MOD 2 +1
  WAIT VBL
LOOP



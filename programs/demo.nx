'** SCROLLING BACKGROUND AND SPRITES DEMO **

BG 0
BG COPY 0,0,32,16 TO 0,0
BG 1
BG COPY 0,16,32,16 TO 0,0

GOSUB NORMALPAL

ON RASTER GOSUB RASTERFX
GAMEPAD 1

BG 1
FONT (7,,,1,),
TEXT 0,0,"SCORE"

'INIT SPRITE
SPRITE.A 0,(6,,,,1)

WATERY=112

JUMP=0
PX=64
PY=96
VY=0

DO
  TICK=TICK+1

  'PLAYER CONTROL
  IF LEFT(0) THEN PX=PX-2
  IF RIGHT(0) THEN PX=PX+2
  IF PY=96 AND (BUTTON TAP(0,0) OR BUTTON TAP(0,1)) THEN VY=-8

  'ANIMATE PLAYER SPRITE
  PY=PY+VY
  IF PY=96 THEN
    VY=0
    FR=130+((TICK/4) MOD 3)*2
  ELSE
    VY=VY+1
    FR=136
  END IF
  SPRITE 0,PX,PY,FR

  'SCROLL MAIN PLANE
  DISPLAY 0,TICK*2,0

  'WATER LEVEL
  WATERY=112+SIN(TICK*0.02)*12

  'FAKE SCORE COUNTER
  NUMBER 15,0,TICK/10,5

  WAIT VBL
LOOP


RASTERFX:
  '3D SCROLLING
  IF RASTER<8 THEN
    DISPLAY 1,0,0
  ELSE IF RASTER<32 THEN
    DISPLAY 1,TICK*3/4,0
  ELSE IF RASTER<56 THEN
    DISPLAY 1,TICK/2,0
  ELSE IF RASTER>WATERY-1 THEN
    OFFS=SIN((RASTER+TICK)*0.1)*4
    DISPLAY 1,TICK*3+OFFS,0
    DISPLAY 0,TICK*2+OFFS,0
  ELSE IF RASTER>56+3*8 THEN
    DISPLAY 1,TICK*3,0
  ELSE
    DISPLAY 1,TICK*(0.5+(RASTER-55)/32),0
  END IF

  'WATER LEVEL
  IF RASTER=0 THEN GOSUB NORMALPAL
  IF RASTER=INT(WATERY) THEN GOSUB WATERPAL

  'SKY COLORS
  IF RASTER=0 THEN PALETTE 0,%110111,,,
  IF RASTER=14 THEN PALETTE 0,%100111,,,
  IF RASTER=28 THEN PALETTE 0,%010111,,,
  IF RASTER=42 THEN PALETTE 0,%000111,,,
RETURN


NORMALPAL:
COPY START(1),LENGTH(1) TO $FF00
RETURN

WATERPAL:
COPY START(5),LENGTH(5) TO $FF00
RETURN



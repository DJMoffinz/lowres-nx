REM BY TIMO KLOSS
REM 2017

REM ======================
REM ** RAM MAP **
REM $A000 - USER CHARS
REM $B000 - USER BG
REM $D002 - USER PALETTES
REM ======================

REM UI COLORS
PALETTE 7,,%111111,%101010,%010101

SELCHAR=1
SELPAGE=0
SELPAL=0
SELPALPAGE=0
SELCOL=1
SELW=0
SELH=0
EDMODE=0
MAPX=0
MAPY=0
DRAGX=0
DRAGY=0

REM ======================

NEWMENU:
GOSUB HIDESPRITES
CHAR 64,(7,1,,,)
BG FILL 0,0 TO 19,15
FONT (7,1,,,),64
TEXT 0,0,"BACKGROUND DESIGNER"
TEXT 1,3,"NEW 20*16"
TEXT 1,5,"NEW 32*16"
TEXT 1,7,"NEW 32*32"
TEXT 1,9,"LOAD"
DO
  IF TOUCH THEN
    Y=INT(TOUCH.Y/8)
    IF Y>=3 AND Y<=4 THEN
      MAPW=20
      MAPH=16
      GOTO BNEW
    END IF
    IF Y>=5 AND Y<=6 THEN
      MAPW=32
      MAPH=16
      GOTO BNEW
    END IF
    IF Y>=7 AND Y<=8 THEN
      MAPW=32
      MAPH=32
      GOTO BNEW
    END IF
    IF Y>=9 AND Y<=10 THEN
      GOSUB BLOADBG
      GOTO CHARMENU 
    END IF
  END IF
  WAIT VBL
LOOP

BNEW:
FILL $B000,2+MAPW*MAPH*2,0
GOTO CHARMENU

REM ======================

MAPEDITOR:
GOSUB HIDESPRITES
COPY $A000,$1000 TO $8000
COPY $D002,32 TO $F200
GOSUB DRAWMAP
GOSUB DRAWTOOLBAR
WAIT 10

OLDX=-1
OLDY=-1

DO
  IF TOUCH THEN
    X=INT(TOUCH.X/8)
    Y=INT(TOUCH.Y/8)
    IF X<>OLDX OR Y<>OLDY THEN

      IF Y>=14 THEN
        IF X>=0 AND X<=1 THEN EDMODE=0
        IF X>=2 AND X<=3 THEN EDMODE=1
        GOSUB CHECKTABBAR
      ELSE
        IF EDMODE=0 THEN
          CHAR ,(SELPAL,0,0,0,0)
          W=MIN(SELW,19-X)
          H=MIN(SELH,13-Y)
          FOR I=0 TO H
            FOR J=0 TO W
              C=SELCHAR+I*16+J
              CHAR C,
              CELL X+J,Y+I
              A=$B002+((Y+I+MAPY)*MAPW+(X+J+MAPX))*2
              POKE A,C
              POKE A+1,SELPAL
            NEXT J
          NEXT I
        ELSE IF EDMODE=1 THEN
          IF OLDX>=0 THEN
            MAPX=MIN(MAX(MAPX-X+DRAGX,0),MAPW-20)
            MAPY=MIN(MAX(MAPY-Y+DRAGY,0),MAPH-14)
            GOSUB DRAWMAP
          END IF
          DRAGX=X
          DRAGY=Y
        END IF
      END IF

      OLDX=X
      OLDY=Y
    END IF
  ELSE
    OLDX=-1
  END IF
  WAIT VBL
LOOP

DRAWMAP:
BG SOURCE $B002,MAPW
BG COPY MAPX,MAPY,20,14 TO 0,0
RETURN

DRAWTOOLBAR:
GOSUB DRAWTABBAR
FONT (7,1,,,),64
TEXT 0,14,"D"
TEXT 2,14,"M"
RETURN

REM ======================

CHARMENU:
PALETTE 7,,%111111,%101010,%010101
PALETTE 6,,%110000,%000000,
GOSUB HIDESPRITES
COPY START(0),LENGTH(0) TO $8000
BG SOURCE START(1)+2,PEEK(START(1))
GOSUB READPAGE
GOSUB READPAL
GOSUB DRAWCHARMENU
WAIT 10

OLDX=-1
OLDY=-1

DO
  IF TOUCH THEN
    X=INT(TOUCH.X/8)
    Y=INT(TOUCH.Y/8)
    IF X<>OLDX OR Y<>OLDY THEN

      GOSUB CHECKTABBAR

      REM ** CHAR **
      IF X>=1 AND X<=16 AND Y>=1 AND Y<=4 THEN
        IF OLDX=-1 THEN
          SELCHAR=((Y-1)*16)+(X-1)+SELPAGE*64
          SELW=0
          SELH=0
          GOSUB DRAWCHARNUM
          DRAGX=X
          DRAGY=Y
        ELSE
          SELW=MAX(X-DRAGX,0)
          SELH=MAX(Y-DRAGY,0)
          GOSUB DRAWCHARNUM
        END IF
      END IF

      REM ** PAGE **
      IF X>=17 AND X<=18 THEN
        IF Y>=1 AND Y<=2 AND SELPAGE>0 THEN
          SELPAGE=SELPAGE-1
          GOSUB READPAGE
          GOSUB DRAWCHARNUM
        END IF
        IF Y>=3 AND Y<=4 AND SELPAGE<3 THEN
          SELPAGE=SELPAGE+1
          GOSUB READPAGE
          GOSUB DRAWCHARNUM
        END IF
      END IF

      REM ** PAL **
      IF Y>=7 AND Y<=8 THEN
        IF X>=1 AND X<=3 THEN
          SELPAL=SELPALPAGE*4+0
          GOSUB DRAWPAL
        END IF
        IF X>=5 AND X<=7 THEN
          SELPAL=SELPALPAGE*4+1
          GOSUB DRAWPAL
        END IF
      END IF
      IF Y>=9 AND Y<=10 THEN
        IF X>=1 AND X<=3 THEN
          SELPAL=SELPALPAGE*4+2
          GOSUB DRAWPAL
        END IF
        IF X>=5 AND X<=7 THEN
          SELPAL=SELPALPAGE*4+3
          GOSUB DRAWPAL
        END IF
      END IF

      REM ** PAL PAGE **
      IF Y>=11 AND Y<=12 THEN
        IF X>=1 AND X<=2 THEN
          SELPALPAGE=0
          GOSUB READPAL
          GOSUB DRAWPAL
        END IF
        IF X>=6 AND X<=7 THEN
          SELPALPAGE=1
          GOSUB READPAL
          GOSUB DRAWPAL
        END IF
      END IF

      REM ** COLOR **
      IF Y>=7 AND Y<=8 THEN
        IF X>=11 AND X<=12 THEN
          SELCOL=0
          GOSUB DRAWSLIDERS
        END IF
        IF X>=13 AND X<=14 THEN
          SELCOL=1
          GOSUB DRAWSLIDERS
        END IF
        IF X>=15 AND X<=16 THEN
          SELCOL=2
          GOSUB DRAWSLIDERS
        END IF
        IF X>=17 AND X<=18 THEN
          SELCOL=3
          GOSUB DRAWSLIDERS
        END IF
      END IF

      REM ** RGB SLIDERS **
      IF Y>=9 AND Y<=12 AND X>=12 AND X<=17 THEN
        V=12-Y
        A=$D002+SELPAL*4+SELCOL
        C=PEEK(A)
        R=INT(C/16)
        G=INT(C/4) MOD 4
        B=C MOD 4
        IF X>=12 AND X<=13 THEN R=V
        IF X>=14 AND X<=15 THEN G=V
        IF X>=16 AND X<=17 THEN B=V
        C=R*16+G*4+B
        POKE A,C
        GOSUB DRAWSLIDERS
        GOSUB READPAL
      END IF

      OLDX=X
      OLDY=Y
    END IF
  ELSE
    OLDX=-1
  END IF
  WAIT VBL
LOOP

DRAWCHARMENU:
BG COPY 0,0,20,16 TO 0,0
FOR I=0 TO 8 STEP 4
  SPRITE.A I+0,(6,0,0,0,)
  SPRITE.A I+1,(6,0,1,0,)
  SPRITE.A I+2,(6,0,0,1,)
  SPRITE.A I+3,(6,0,1,1,)
NEXT I
FOR I=12 TO 14
  SPRITE.A I,(7,0,0,0,0)
  SPRITE.S I,(1,0)
NEXT I
GOSUB DRAWTABBAR
GOSUB DRAWCHARNUM
GOSUB DRAWPAGE
GOSUB DRAWPAL
RETURN

DRAWCHARNUM:
FONT (7,1,,,),64
TEXT 1,5,"#"
NUMBER 2,5,SELCHAR,3
NUMBER 6,5,SELW+1,1
TEXT 7,5,"*"
NUMBER 8,5,SELH+1,1
NUMBER 14,5,SELPAGE+1,1
TEXT 15,5,"/4"
IF SELCHAR>=SELPAGE*64 AND SELCHAR<SELPAGE*64+64 THEN
  N=SELCHAR MOD 64
  SX=32+8+(N MOD 16)*8
  SY=32+8+INT(N/16)*8
  SPRITE 0,SX-1,SY-1,36
  SPRITE 1,SX+SELW*8+1,SY-1,36
  SPRITE 2,SX-1,SY+SELH*8+1,36
  SPRITE 3,SX+SELW*8+1,SY+SELH*8+1,36
  HIDECOUNT=60
ELSE
  GOSUB HIDESELCHAR
END IF
RETURN

HIDESELCHAR:
SPRITE 0,0,0,
SPRITE 1,0,0,
SPRITE 2,0,0,
SPRITE 3,0,0,
RETURN

DRAWPAGE:
CHAR ,(0,0,,,)
CI=192
FOR CY=1 TO 4
  FOR CX=1 TO 16
    CHAR CI,
    CELL CX,CY
    CI=CI+1
  NEXT CX
NEXT CY
RETURN

READPAGE:
COPY $A000+SELPAGE*$400,$400 TO $8C00
RETURN

READPAL:
COPY $D002+SELPALPAGE*16,16 TO $F200
RETURN

DRAWPAL:
FONT (7,1,,,),64
TEXT 3,11,"#"
NUMBER 4,11,SELPAL,1
NUMBER 3,12,SELPALPAGE+1,1
TEXT 4,12,"/2"
C=SELPALPAGE*4
NUMBER 1,8,C+0,1
NUMBER 5,8,C+1,1
NUMBER 1,10,C+2,1
NUMBER 5,10,C+3,1
GOSUB DRAWSLIDERS
P=(SELPAL MOD 4)
FOR CY=1 TO 4
  FOR CX=1 TO 16
    POKE $9001+(CY*32+CX)*2,P
  NEXT CX
NEXT CY
FOR CY=7 TO 8
  FOR CX=11 TO 18
    POKE $9001+(CY*32+CX)*2,P
  NEXT CX
NEXT CY
RETURN

DRAWSLIDERS:
C=PEEK($D002+SELPAL*4+SELCOL)
R=INT(C/16)
G=INT(C/4) MOD 4
B=C MOD 4
SPRITE 12,32+12*8,32+(12-R)*8,52
SPRITE 13,32+14*8,32+(12-G)*8,52
SPRITE 14,32+16*8,32+(12-B)*8,52
RETURN

REM ======================

DISKMENU:
GOSUB HIDESPRITES
GOSUB DRAWDISKMENU
WAIT 10

OLDX=-1
OLDY=-1

DO
  IF TOUCH THEN
    X=INT(TOUCH.X/8)
    Y=INT(TOUCH.Y/8)
    IF X<>OLDX OR Y<>OLDY THEN

      GOSUB CHECKTABBAR

      IF X>=1 AND X<=8 THEN
        IF Y>=2 AND Y<=3 THEN GOSUB BLOADCHR
        IF Y>=4 AND Y<=5 THEN GOSUB BLOADBG
        IF Y>=6 AND Y<=7 THEN GOSUB BSAVEBG
        IF Y>=8 AND Y<=9 THEN GOSUB BLOADPAL
        IF Y>=10 AND Y<=11 THEN GOSUB BSAVEPAL
        IF Y>=12 AND Y<=13 THEN GOTO NEWMENU
      END IF

      OLDX=X
      OLDY=Y
    END IF
  END IF
  WAIT VBL
LOOP

DRAWDISKMENU:
CHAR 64,(7,1,,,)
BG FILL 0,0 TO 19,15
GOSUB DRAWTABBAR
FONT (7,1,,,),64
TEXT 0,0,"DISK MENU"
TEXT 1,2,"LOAD(CH)"
TEXT 1,4,"LOAD(BG)"
TEXT 1,6,"SAVE(BG)"
TEXT 1,8,"LOAD(PAL)"
TEXT 1,10,"SAVE(PAL)"
TEXT 1,12,"NEW(BG)"
RETURN

BLOADBG:
LOAD "#1",$B000
MAPW=PEEK($B000)
MAPH=PEEK($B001)
RETURN

BSAVEBG:
POKE $B000,MAPW
POKE $B001,MAPH
SAVE "#1:BG DEFAULT",$B000,2+MAPW*MAPH*2
RETURN

BLOADCHR:
LOAD "#0",$A000
RETURN

BLOADPAL:
LOAD "#2",$D002
RETURN

BSAVEPAL:
SAVE "#2:PAL DEFAULT",$D002,32
RETURN

REM ======================

DRAWTABBAR:
CHAR 64,(7,1,,,)
BG FILL 0,14 TO 19,15
FONT (7,1,,,),64
TEXT 12,14,"ED"
TEXT 12,15,"IT"
TEXT 15,14,"ME"
TEXT 15,15,"NU"
TEXT 18,14,"DI"
TEXT 18,15,"SK"
RETURN

CHECKTABBAR:
IF Y>=14 THEN
  IF X>=12 AND X<=13 THEN GOTO MAPEDITOR
  IF X>=15 AND X<=16 THEN GOTO CHARMENU
  IF X>=18 AND X<=19 THEN GOTO DISKMENU
END IF
RETURN

HIDESPRITES:
FOR I=0 TO 63
  SPRITE I,0,0,0
  SPRITE.A I,0
  SPRITE.S I,0
NEXT I
RETURN


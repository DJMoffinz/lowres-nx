DIM ARR(4)
FOR I=0 TO 4
 ARR(I)=I+1
NEXT I
A=42
B=11
PRINT "BEFORE:",A,B
CALL TEST
PRINT "AFTER: ",A,B
CALL PRINTARR

SUB TEST
 SHARED A,ARR()
 B=22
 A=A+1
 PRINT "SUB:   ",A,B
 FOR I=0 TO 4
  ARR(I)=ARR(I)+1
 NEXT I
 CALL PRINTARR
 IF A<=44 THEN CALL TEST
END SUB

SUB PRINTARR
 SHARED ARR()
 FOR I=0 TO 4
  PRINT ARR(I),
 NEXT I
 PRINT
END SUB

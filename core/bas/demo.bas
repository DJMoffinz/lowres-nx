REM LITTLE TEST PROGRAM

DIM ARR$(10,2)
FOR I=0 TO 10
    ARR$(I,0)="HELLO "+STR$(I)
    ARR$(I,1)="LOWRES"
NEXT I
FOR I=0 TO 10
    FOR J=0 TO 2
        PRINT ARR$(I,J)
    NEXT J
NEXT I

GOSUB HELLO

PRINT ASC("A"),ASC("Z"),ASC("HELLO")
PRINT CHR$(65),CHR$(90)
PRINT LEN("HELLO"),LEN("")
GOSUB HELLO
END

WHAT:
PRINT "WHAT? (GOSUB)"
RETURN

HELLO:
PRINT "HELLO (GOSUB)"
GOSUB WHAT
RETURN
